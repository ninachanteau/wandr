<% if @activity.status == "Booked" %>
  document.querySelector("[data-activity-id='<%= @activity.id %>']").innerHTML = "<%= j render 'shared/wishlist-icon'  %>"
<% else %>
  document.querySelector("[data-activity-id='<%= @activity.id %>']").innerHTML = "<%= j render 'shared/booked-icon'  %>"
<% end %>
