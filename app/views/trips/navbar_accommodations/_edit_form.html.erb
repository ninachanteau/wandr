<div class="modal-header">
  <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
  <h4 class="modal-title text-center" id="myModalLabel">Add Accommodation</h4>
</div>
<div class="modal-body">
<%= simple_form_for accommodation, url: trips_navbar_accommodation_path(accommodation), method: :patch do |f| %>
<div class="new-container">
<div class="container">
  <div class="row">
    <div class="col-md-4">
        <%= f.input :name %>
        <%= f.input :address %>
        <%= f.input :phone_number %>
        <%= f.input :email %>
        <%= f.input :url %>
        <%= f.input :photo %>
        <%= f.select :trip, options_for_select(trips) %>
        <%= f.button :submit, class: "btn btn-info btn-form", style: "margin: 20px" %>
      <% end %>
    </div>
  </div>
</div>
</div>
</div>
