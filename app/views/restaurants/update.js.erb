<% if @restaurant.status == "Booked" %>
  document.querySelector("[data-restaurant-id='<%= @restaurant.id %>']").innerHTML = "<%= j render 'shared/wishlist-icon'  %>"
<% else %>
  document.querySelector("[data-restaurant-id='<%= @restaurant.id %>']").innerHTML = "<%= j render 'shared/booked-icon'  %>"
<% end %>
