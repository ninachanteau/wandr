<% if @accommodation.status == "Booked" %>
  document.querySelector("[data-accommodation-id='<%= @accommodation.id %>']").innerHTML = "<%= j render 'shared/wishlist-icon'  %>"
<% else %>
  document.querySelector("[data-accommodation-id='<%= @accommodation.id %>']").innerHTML = "<%= j render 'shared/booked-icon'  %>"
<% end %>
