
function loadAccommodationsWrapper(innerHTML) {
  const wrapper = document.querySelector('.wrapper');
  wrapper.innerHTML = innerHTML;
};

loadAccommodationsWrapper('<%= j render "index", accommodations: @accommodations, trip_participants: @trip_participants, trip_dates: @trip_dates, trip: @trip, accommodation: @accommodation, my_accommodations: @my_accommodations %>');

function removeNotification() {
  const notification = document.getElementById('accommodations-notif');
  if (notification) {
    notification.remove();
  }
}

removeNotification()

